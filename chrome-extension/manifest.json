{
  "manifest_version": 2,
  "name": "よくないね。",
  "version": "0.0.3",
  "description": "いいね　だけじゃ　だめだよ",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "http://qiita.com/*",
    "https://qiita.com/*"
  ],
  "options_page": "options.html",
  "permissions": [
    "storage"
  ],
  "web_accessible_resources": [
    "options.html"
  ],
  "author": "mzyy94",
  "content_scripts": [{
    "matches": [ "http://qiita.com/*/items/*" ],
    "js": [ "index.js" ],
    "run_at": "document_start"
  }]
}
